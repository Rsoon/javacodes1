class Factor {
boolean isFactor(int a, int b) {
if( ( b % a) == 0) return true; 
else return false; 
}
} 
class isFact {
public static void main(String args[]) {
Factr X = new Factor(); 
if(X.isFactor(2, 20)) System.out.println("2 is factor"); 
